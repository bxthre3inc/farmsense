import { useState, useEffect } from 'react';
import {
  TrendingUp, Droplets, Shield, Map, Activity, Users, DollarSign,
  Calendar, ArrowUpRight, ArrowDownRight, Bell, Settings, LogOut,
  FileText, Download, Filter, Search, ChevronDown, Globe,
  Zap, BarChart3, PieChart, Award
} from 'lucide-react';
import { removeApiKey } from '../shared/api';
import Login from './Login';

// Types
interface WaterRight {
  id: string;
  owner: string;
  acre_feet: number;
  priority_date: string;
  district: string;
  status: 'active' | 'pending' | 'suspended';
  usage_ytd: number;
}

interface SLVMetric {
  year: number;
  aquifer_level: number;
  recharge_rate: number;
  sustainability_score: number;
  compliance_rate: number;
}

interface InvestmentOpportunity {
  id: string;
  name: string;
  type: 'water_right' | 'land' | 'tech' | 'carbon';
  value: number;
  projected_return: number;
  risk_level: 'low' | 'medium' | 'high';
  description: string;
}

const mockWaterRights: WaterRight[] = [
  { id: 'WR-001', owner: 'Valley Farms LLC', acre_feet: 1250, priority_date: '1882-03-15', district: 'Subdistrict 1', status: 'active', usage_ytd: 840 },
  { id: 'WR-002', owner: 'Rio Grande Associates', acre_feet: 2100, priority_date: '1895-07-22', district: 'Subdistrict 2', status: 'active', usage_ytd: 1520 },
  { id: 'WR-003', owner: 'High Desert Ag', acre_feet: 800, priority_date: '1901-11-08', district: 'Subdistrict 1', status: 'pending', usage_ytd: 0 },
];

const mockSLVHistory: SLVMetric[] = [
  { year: 2020, aquifer_level: 45.2, recharge_rate: 12.5, sustainability_score: 68, compliance_rate: 94 },
  { year: 2021, aquifer_level: 44.8, recharge_rate: 11.8, sustainability_score: 67, compliance_rate: 92 },
  { year: 2022, aquifer_level: 44.1, recharge_rate: 10.2, sustainability_score: 65, compliance_rate: 89 },
  { year: 2023, aquifer_level: 43.5, recharge_rate: 9.8, sustainability_score: 64, compliance_rate: 91 },
  { year: 2024, aquifer_level: 42.9, recharge_rate: 11.5, sustainability_score: 66, compliance_rate: 93 },
  { year: 2025, aquifer_level: 43.2, recharge_rate: 12.1, sustainability_score: 68, compliance_rate: 95 },
];

const mockOpportunities: InvestmentOpportunity[] = [
  { id: 'OPP-001', name: 'Conservation Easement - Block 12', type: 'water_right', value: 450000, projected_return: 12.5, risk_level: 'low', description: 'Senior water right with 800 acre-feet annual allocation. Farm transitioning to dryland crops.' },
  { id: 'OPP-002', name: 'Aquifer Recharge Project', type: 'land', value: 1200000, projected_return: 8.2, risk_level: 'medium', description: '160-acre recharge site with state-matched funding. 15-year water banking agreement.' },
  { id: 'OPP-003', name: 'Precision Ag Tech Stack', type: 'tech', value: 850000, projected_return: 22.0, risk_level: 'high', description: 'Full VRI, soil sensing, and automation deployment across 3,000 acres.' },
];

const InvestorDashboard = () => {
  const [isAuthenticated, setIsAuthenticated] = useState(false);
  const [activeTab, setActiveTab] = useState('overview');

  if (!isAuthenticated) {
    return <Login onLogin={() => setIsAuthenticated(true)} />;
  }

  return (
    <div className="min-h-screen bg-slate-950 flex">
      {/* Sidebar */}
      <aside className="w-64 bg-slate-900 border-r border-slate-800 flex flex-col">
